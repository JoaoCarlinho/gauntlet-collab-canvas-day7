{
  "build": {
    "builder": "RAILPACK"
  },
  "deploy": {
    "startCommand": "cd backend && ./start.sh",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 100,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  },
  "secrets": {
    "CORS_ORIGINS": "https://collabcanvas-mvp-day7.vercel.app,https://*.vercel.app"
  }
}
